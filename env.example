# Environment Variables for Jamie Application
# Copy this file to .env and update with your actual values

# Google Cloud Project Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-west1

# Service Account (optional - will be auto-generated if not set)
ORCHESTRATOR_SERVICE_ACCOUNT=jamie-orchestrator@your-project-id.iam.gserviceaccount.com

# Storage Configuration
BASE_BUCKET=jamie-storage-bucket

# API Keys (for local development)
GEMINI_API_KEY=your-gemini-api-key
PLACES_API_KEY=your-places-api-key
